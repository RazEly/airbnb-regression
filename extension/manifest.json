{
  "manifest_version": 3,
  "name": "Airbnb Price Predictor",
  "version": "2.0",
  "description": "ML-powered price predictions with stoplight indicators and calendar coloring for Airbnb listings",
  "content_scripts": [
    {
      "matches": [
        "*://*.airbnb.com/*",
        "*://*.airbnb.co.uk/*",
        "*://*.airbnb.ca/*",
        "*://*.airbnb.de/*",
        "*://*.airbnb.fr/*",
        "*://*.airbnb.es/*",
        "*://*.airbnb.it/*",
        "*://*.airbnb.com.au/*"
      ],
      "js": [
        "overlay.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "http://127.0.0.1:5001/*"
  ],
  "permissions": []
}